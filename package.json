{
  "name": "eemeli",
  "version": "1.0.0",
  "description": "Personal npm card for Eemeli Aro (@eemeli)",
  "main": "dist/card.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/eemeli/npm-card.git"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && webpack"
  },
  "bin": {
    "eemeli": "dist/card.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "eemeli",
    "card",
    "npm",
    "npm card",
    "npx",
    "npx card",
    "business card"
  ],
  "author": "Eemeli Aro",
  "license": "ISC",
  "devDependencies": {
    "boxen": "^2.1.0",
    "chalk": "^2.4.1",
    "chalker": "^1.0.0",
    "lodash.get": "^4.4.2"
  },
  "myCard": {
    "info": {
      "name": "Eemeli Aro",
      "handle": "eemeli",
      "twitter": "eemeli_aro",
      "npm": "eemeli",
      "github": "eemeli",
      "web": "https://eemeli.org/talks",
      "work": "Vincit"
    },
    "data": [
      "          <white>{{name}}</><green> / </><cyan>{{handle}}</>",
      "",
      {
        "label": "<blue>Work</>",
        "text": "Library developer at <lightblue>{{work}}</>"
      },
      "",
      {
        "label": "Twitter",
        "text": "https://twitter.com/<cyan>{{twitter}}</>",
        "when": "{{twitter}}"
      },
      {
        "label": "npm",
        "text": "https://www.npmjs.com/<red>~{{npm}}</>",
        "when": "{{npm}}"
      },
      {
        "label": "GitHub",
        "text": "https://github.com/<green>{{github}}</>",
        "when": "{{github}}"
      },
      {
        "label": "LinkedIn",
        "text": "https://www.linkedin.com/in/<blue>{{linkedin}}</>",
        "when": "{{linkedin}}"
      },
      {
        "label": "Web",
        "text": "<cyan>{{web}}</>",
        "when": "{{web}}"
      },
      "",
      {
        "label": "Card",
        "text": "<red>npx</> {{_packageName}}"
      }
    ]
  }
}
